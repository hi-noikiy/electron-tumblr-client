{
  "name": "electron-tumblr-client",
  "version": "0.1.1",
  "description": "A desktop client application for Tumblr. This is built on Electron.",
  "main": "main.js",
  "scripts": {
    "package": "electron-packager ./ electron-tumblr-client --platform=darwin,linux,win32 --arch=x64 --version=0.31.2 --out=pkgs/v0.1.1 --ignore=pkgs/*",
    "upload": "ghr v0.1.1 pkgs/v0.1.1",
    "zip": "npm run zip-darwin && npm run zip-linux && npm run zip-win32",
    "zip-darwin": "cd pkgs/v0.1.1 && zip -r electron-tumblr-client-darwin-x64.zip electron-tumblr-client-darwin-x64",
    "zip-linux": "cd pkgs/v0.1.1 && zip -r electron-tumblr-client-linux-x64.zip electron-tumblr-client-linux-x64",
    "zip-win32": "cd pkgs/v0.1.1 && zip -r electron-tumblr-client-win32-x64.zip electron-tumblr-client-win32-x64"
  },
  "keywords": [],
  "author": "yskm",
  "license": "MIT",
  "dependencies": {
    "babel": "^5.8.23",
    "font-awesome": "^4.4.0",
    "materialize-css": "^0.97.1",
    "react": "^0.13.3"
  },
  "devDependencies": {
    "electron-packager": "^5.0.2",
    "electron-prebuilt": "^0.31.2",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.2.1",
    "gulp-watch": "^4.3.5"
  }
}
